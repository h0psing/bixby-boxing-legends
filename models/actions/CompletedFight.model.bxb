action (CompletedFight) {
  type (Search)
  description (Compares the user's answer to the correct answer and updates your score and move to the next question)
  collect {
    input (answer) {
      type (Answer)
      min (Required) max (One)
    }
    input (quiz) {
      type (Match)
      min (Optional) max (One)
    }
  }
  output (Match)
}
